  <item>
    <name>Change QWERTY to Colemak layout</name>
    <identifier>remap.qwerty2colemak_custom</identifier>
    <include path="/Applications/KeyRemap4MacBook.app/Contents/Resources/include/checkbox/keyboard_layouts/common/keypad_redefinition.xml" />
